<!-- views/skills/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Skills</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach((skill) => { %>
      <li>
        <a href="/skills/<%= skill.id %>"><%= skill.name %></a>
      </li>
    <% }); %>
  </ul>
</body>
</html>

